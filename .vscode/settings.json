{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.words": [
    "emojify",
    "hasura",
    "tempfile",
    "todos",
    "vercel"
  ],
  "eslint.onIgnoredFiles": "warn",
  "eslint.probe": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown",
    "graphql"
  ],
  "editor.formatOnSave": true,
  "editor.tabSize": 2
}
